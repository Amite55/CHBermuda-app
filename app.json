{
  "expo": {
    "name": "bermuda",
    "slug": "bermuda",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/splashIcon.png",
    "scheme": "bermuda",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "config": {
        "googleMapsApiKey": "AIzaSyA3swfjp2TwJ-AVCVuaPEtLUB8qKmrZOLA"
      },
      "supportsTablet": true
    },
    "android": {
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyA3swfjp2TwJ-AVCVuaPEtLUB8qKmrZOLA"
        }
      },
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/androidAppIcon.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.madhob.bermuda"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/androidAppIcon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-font",
      "expo-build-properties",
      [
        "expo-maps",
        {
          "requestLocationPermission": true
          // enable for ios
          // "locationPermission": "Allow $(PRODUCT_NAME) to use your location"
        }
      ]
      
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "afee3cd5-6687-4c7a-aefe-073baa744186"
      }
    }
  }
}
